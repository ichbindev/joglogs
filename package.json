{
  "name": "joglog",
  "version": "2.0.0",
  "description": "Running Coach Webapp",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "lint": "eslint --quiet .",
    "fix": "eslint --fix ."
  },
  "author": "Perry Williams, Sager Williams (no relation), Chris Mlinac, Aaron Phillips",
  "license": "ISC",
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "animate.css": "^3.7.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.1",
    "express-session": "^1.17.0",
    "googleapis": "^44.0.0",
    "if-env": "^1.0.4",
    "jquery": "^3.5.0",
    "moment": "^2.24.0",
    "mysql2": "^1.6.5",
    "nodemailer": "^6.4.16",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "react-animate-on-scroll": "^2.1.5",
    "react-loader": "^2.4.6",
    "react-loader-spinner": "^3.1.5",
    "react-spinners": "^0.6.1",
    "sequelize": "^5.21.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1"
  }
}
